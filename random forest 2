   ],
   "source": [
    "import pandas as pd\n",
    "\n",
    "# Load datasets\n",
    "malaria_df = pd.read_csv(\"DatasetAfricaMalaria.csv\")\n",
    "export_df = pd.read_csv(\"Data_Export.csv\", encoding='latin1')\n",
    "national1_df = pd.read_csv(\"National_unit1.csv\")\n",
    "national2_df = pd.read_csv(\"National_unit2.csv\")\n",
    "\n",
    "# Standardize Country Names\n",
    "malaria_df['Country Name'] = malaria_df['Country Name'].str.strip().str.upper()\n",
    "export_df['Country Name'] = export_df['Country Name'].str.strip().str.upper()\n",
    "national1_df['Name'] = national1_df['Name'].str.strip().str.upper()\n",
    "national2_df['Name'] = national2_df['Name'].str.strip().str.upper()\n",
    "\n",
    "# Rename 'Name' to 'Country Name' for consistency\n",
    "national1_df.rename(columns={'Name': 'Country Name'}, inplace=True)\n",
    "national2_df.rename(columns={'Name': 'Country Name'}, inplace=True)\n",
    "\n",
    "# Merge export_df with malaria_df on Country Name and Year\n",
    "merged_df = pd.merge(export_df, malaria_df, on=['Country Name', 'Year'], how='outer')\n",
    "\n",
    "# Merge with national1_df\n",
    "merged_df = pd.merge(merged_df, national1_df, on=['Country Name', 'Year'], how='outer', suffixes=('', '_nat1'))\n",
    "\n",
    "# Merge with national2_df\n",
    "merged_df = pd.merge(merged_df, national2_df, on=['Country Name', 'Year'], how='outer', suffixes=('', '_nat2'))\n",
    "\n",
    "# Display result\n",
    "print(merged_df.info())\n",
    "print(merged_df.head())\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "eee23419-4352-4a75-b045-47df380995d6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Malaria Risk Class Distribution:\n",
      "Malaria_Risk\n",
      "None      1058\n",
      "Low        634\n",
      "High       570\n",
      "Medium     517\n",
      "Name: count, dtype: int64\n"
     ]
    }
   ],
   "source": [
    "# Convert incidence column to numeric (in case itâ€™s read as object due to commas or spaces)\n",
    "merged_df['Incidence of malaria (per 1,000 population at risk)'] = pd.to_numeric(\n",
    "    merged_df['Incidence of malaria (per 1,000 population at risk)'], errors='coerce'\n",
    ")\n",
    "\n",
    "# Define a function to classify malaria risk\n",
    "def classify_risk(incidence):\n",
    "    if pd.isnull(incidence):\n",
    "        return None\n",
    "    elif incidence < 100:\n",
    "        return 'Low'\n",
    "    elif incidence < 300:\n",
    "        return 'Medium'\n",
    "    else:\n",
    "        return 'High'\n",
    "\n",
    "# Apply classification function\n",
    "merged_df['Malaria_Risk'] = merged_df['Incidence of malaria (per 1,000 population at risk)'].apply(classify_risk)\n",
    "\n",
    "# Check class distribution\n",
    "print(\"Malaria Risk Class Distribution:\")\n",
    "print(merged_df['Malaria_Risk'].value_counts(dropna=False))\n"
   ]
  },
